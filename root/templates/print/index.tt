        <input type="text" name="extra_column" placeholder="extra column">
        <input type="text" name="extra_column" placeholder="extra column">
        <input type="text" name="extra_column" placeholder="extra column">
        <input type="submit" value="Project print view">
    </fieldset>
</form>
